{
  "name": "HealthDashboardApp",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run server",
    "client": "cd client && npm run start",
    "server": "nodemon --max-old-space-size=4096 server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "devTest": "set DEBUG=express:* && concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prodTest": "set NODE_ENV=production && npm run server"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.18.3",
    "concurrently": "^3.5.0"
  },
  "dependencies": {
    "@cloudant/cloudant": "^2.3.0",
    "@material-ui/core": "^3.2.2",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "moment": "^2.22.2",
    "mongodb": "^3.1.3",
    "node-excel-export": "^1.4.4",
    "nodemon": "^1.18.3",
    "react-text-mask": "^5.4.3",
    "twilio": "^3.23.1",
    "uuid": "^3.3.2"
  }
}
